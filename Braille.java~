import java.util.Scanner;
import java.io.File;
public class Braille{
  public static void main(String[] args) throws java.io.FileNotFoundException{ 
    Scanner file = new Scanner(new File("Braille1.txt"));
    
    String letter = ""; //this variable will be used later
    //Seperate every 3 lines
    for (int h = 0; h < 3; h++) {
      String line = file.next();
      String[] arr = new String[3];
      arr[h] = line;  //add each line to the array which makes up the words
    }
      for (int k = 0; k < line.length/*-2hereMaybe*/(); k=k+2) {  //skips 2 because of braille structure
        for (int i = 0; i < 2; i++) { //goes through the lines which were added to the array
          letter = letter + /*null pointer exception here*/arr[i].substring(k,k+2); //adds up each substring 
        }
        System.out.println(letter);
      }
      
  }
}